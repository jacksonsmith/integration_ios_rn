{
  "name": "SellingCartApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build-ios-bundle": "react-native bundle --entry-file='index.js' --bundle-output='./ios/Pod/Assets/index.ios.jsbundle' --dev=false --platform='ios' --assets-dest='./ios/Pod/Assets/'",
    "build-android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/SellingCartLibrary/src/main/assets/index.android.bundle --assets-dest android/SellingCartLibrary/src/main/res",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.64.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/runtime": "^7.10.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.5.0",
    "jest": "^26.1.0",
    "metro-react-native-babel-preset": "^0.61.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
